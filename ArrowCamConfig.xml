<?xml version="1.0" encoding="utf-16"?>
<config>
  <!--If you want to reset the configuration to default values just delete this file.-->
  <!--How long the camera will stay active when lifting the ArrowCam key.-->
  <stopWatchingDelay>0.2</stopWatchingDelay>
  <!--Enables (True) / Disables (False) the slow motion after a hit.-->
  <enableSlowMotion>True</enableSlowMotion>
  <!--Maximum time that the slow motion will be active.-->
  <slowMotionTime>3</slowMotionTime>
</config>